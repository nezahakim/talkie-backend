{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/src/socket.io.js"
    }
  ],
  "functions": {
    "src/app.js": {
      "maxDuration": 10
    }
  }
}